{
  "env": {
    "commonjs": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "no-tabs": "off",
    "no-console": "off",
    "comma-dangle": [
      "warn",
      {
        "functions": "never",
        "objects": "always-multiline",
        "arrays": "always-multiline"
      }
    ]
  }
}
